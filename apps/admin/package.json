    "antd-mobile": "^5.39.0",
    "antd-mobile-icons": "^0.3.0",
    "array-move": "^4.0.0",
    "clsx": "^2.1.1",
    "motion": "^12.23.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.6.3",
