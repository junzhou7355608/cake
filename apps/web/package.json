{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm clean && next dev --port 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "lint": "eslint --max-warnings 0",
    "check-types": "next typegen && tsc --noEmit",
    "scan": "next dev & npx react-scan@latest localhost:3000",
