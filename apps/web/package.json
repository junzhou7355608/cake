    "gen:icons": "npx @svgr/cli -d ./components/icons/custom ./public/icons/flags"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@repo/hooks": "workspace:*",
    "@repo/models": "workspace:*",
    "@repo/services": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "jotai": "^2.16.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.1",
    "next-intl": "^4.6.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
